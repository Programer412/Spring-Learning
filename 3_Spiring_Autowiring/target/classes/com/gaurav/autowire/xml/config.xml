<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
         xmlns:p="http://www.springframework.org/schema/p"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

<bean name="add1" class="com.gaurav.autowire.xml.Address">
<property name="city">
<value>Shimla</value>
</property>
<property name="state">

<value>Himachal</value>

</property>
</bean>

<bean name="add2" class="com.gaurav.autowire.xml.Address">
<property name="city">
<value>palampur</value>
</property>
<property name="state">

<value>Kangra</value>

</property>
</bean>

<bean name="add3" class="com.gaurav.autowire.xml.Address" primary="true">
<property name="city">
<value>Mandi</value>
</property>
<property name="state">

<value>Himachal</value>
</property>
</bean>

