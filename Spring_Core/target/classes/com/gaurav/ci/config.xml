<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
         xmlns:p="http://www.springframework.org/schema/p"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
<bean name="ce" class="com.gaurav.ci.Certificate">
<constructor-arg value="1"></constructor-arg>
<constructor-arg value="java"></constructor-arg>

</bean> 

<bean name="st" class="com.gaurav.ci.Student">
<constructor-arg value="1" type="int" ></constructor-arg>
<constructor-arg value="gaurav" ></constructor-arg>
<constructor-arg value="India"></constructor-arg>
<constructor-arg>
<list>
<value>java</value>
<value>c++</value>
</list>
</constructor-arg>
<constructor-arg ref="ce"></constructor-arg>

</bean>


</beans>